{
  "name": "coding-challenge-guidelines",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:style": "stylelint '**/*.{css,less,scss,sass,tsx}'",
    "preview": "vite preview",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,scss,less,css,json,md,yml,yaml}\"",
    "test": "jest",
    "test:cov": "jest --coverage",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:e2e": "start-server-and-test start :3000 cy:open-e2e"
  },
  "dependencies": {
    "chart.js": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12"
  },
  "devDependencies": {
    "@swc/core": "^1.4.2",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.6.5",
    "eslint": "^8.56.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "less": "^4.2.0",
    "postcss-scss": "^4.0.9",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  }
}
